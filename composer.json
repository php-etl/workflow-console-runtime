    "autoload-dev": {
        "psr-4": {
            "functional\\Kiboko\\Component\\Runtime\\Workflow\\": "tests/unit/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.1.x-dev"
        }
    }
