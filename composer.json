{
    "name": "php-etl/workflow-console-runtime",
    "description": "This library allows you to use the workflow console runtime",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kiboko SAS",
            "homepage": "http://kiboko.fr"
        },
        {
            "name": "Gr√©gory Planchat",
            "email": "gregory@kiboko.fr"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "symfony/console": "^5.2 || ^6.0",
        "php-etl/pipeline-contracts": "^0.4@dev",
        "php-etl/pipeline": "^0.5@dev",
        "php-etl/console-state": "^0.2@dev",
        "php-etl/pipeline-console-runtime": "^0.2@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5 || ^10.0",
        "rector/rector": "^0.15",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.0",
        "infection/infection": "^0.26.18"
    },
    "autoload": {
        "psr-4": {
            "Kiboko\\Component\\Runtime\\Workflow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "functional\\Kiboko\\Component\\Runtime\\Workflow\\": "tests/unit/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "allow-plugins": {
            "infection/extension-installer": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.2.x-dev",
            "dev-feature/qualityflow-imrovments": "0.2.x-dev"
        }
    }
}
