    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "symfony/console": "^5.2",
        "php-etl/pipeline-contracts": "^0.3.0",
        "php-etl/pipeline": "^0.4.0",
        "php-etl/console-state": "^0.1.0",
        "php-etl/pipeline-console-runtime": "^0.1.0"
    },
    "autoload": {
        "psr-4": {
            "Kiboko\\Component\\Runtime\\Workflow\\": "src/"
        }
